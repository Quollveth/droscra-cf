{
	"compilerOptions": {
    "allowImportingTsExtensions": true,
		"target": "es2021",
	    "lib": ["ES2021", "DOM", "DOM.Iterable"],
		"jsx": "react-jsx",
		"module": "es2022",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowJs": true,
		"checkJs": false,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
